date="date +%d.%m.%Y"
hour="date +%H.%M.%S"

tempPath="/tmp/ArduinoCLI"
commandPath="$tempPath/output"
logPath="{fileDirAbsPath}/Logs/$($date)"

if [ ! -d "$tempPath" ]
then
  mkdir -p "$tempPath"
fi

if [ ! -d "$commandPath" ]
then
  mkdir -p "$commandPath"
fi

if [ ! -d "$logPath" ]
then
  mkdir -p "$logPath"
fi

arduino-cli monitor --fqbn {ArduinoBoard} --port {ArduinoPort} --no-color
